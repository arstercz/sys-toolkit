#!/usr/bin/env perl
=head1 NAME

sys-mysql-error - Explain MySQL error codes

=cut

=head1 SYNOPSIS

    use sys-mysql-error --help for more info.

    options:
       code|c: mysql error code, eg: 1001
       help|h: print help info.

    read more:
       like the mysql perror command.

=cut

use strict;
use warnings;
use Getopt::Long;

my $code;
my $help;

GetOptions(
    "code|c=s"  => \$code,
    "help|h!"   => \$help,
);

if($help) {
    usage($0);
}

sub usage {
    my $name = shift;
    system("perldoc $name");
    exit 0;
}

unless ($code) {
    print "no code value, use --help for more info\n";
    exit 1;
}

print  MySQL::errors->new( 
          code => $code,
       )->get_code();

=head1 AUTHOR

zhe.chen <chenzhe07\@gmail.com>

=head1 CHANGELOG

v0.1.0 version

=cut

package MySQL::errors;
# MySQL error list based on MySQL Server 5.7.14-8.
# all the list from the source sql/share/errmsg-utf8.txt
use strict;
use warnings FATAL => 'all';
use English qw(-no_match_vars);
use Carp qw(croak);
use Data::Dumper;

sub new {
    my ( $class, %args ) = @_;
    my @required_args = qw(code);
    foreach my $arg (@required_args) {
       die "I need a $arg argument" unless $args{$arg};
    }
    my $self = {
       code => undef,
    };
    bless $self, $class;
    $self->{code} = $args{'code'};
    return $self;
}

sub get_code {
    my $self = shift;
    my $ref_err = _get_info($self->{code});
    unless (defined($ref_err)) {
        return "Illegal error code: "
               . $self->{code} . "\n";
    }
    return "MySQL error code "
           . $self->{code}
           . " (" . $ref_err->{err} . "): "
           . $ref_err->{msg} . "\n";
}

